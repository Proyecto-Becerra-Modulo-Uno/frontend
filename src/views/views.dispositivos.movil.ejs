    <!-- partial navbar -->
    <%- include("partials/partial.navbar.usuarios.ejs") %>

    <div class="devices_movil row">
        <i class="devices_movil bi bi-arrow-left-short col-sm-1 col-md-1 col-lg-1 text-center display-6 pt-3"></i>
